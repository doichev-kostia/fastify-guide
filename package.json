{
  "name": "fastify-guide",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "build": "swc src --out-dir build",
    "build:clean": "rm -rf build && pnpm run build",
    "watch": "swc src -d build --watch",
    "start:js": "pnpm exec fastify start -l info ./build/app.js",
    "dev:js": "pnpm exec fastify start -w -l info -P ./build/app.js",
    "start": "pnpm exec tsx --no-cache --tsconfig ./tsconfig.json ./src/main.ts",
    "dev": "pnpm exec tsx watch --tsconfig ./tsconfig.json ./src/main.ts",
    "lint": "pnpm exec standard",
    "lint:fix": "pnpm exec standard --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.68",
    "@swc/helpers": "^0.5.1",
    "@types/node": "^20.3.3",
    "chokidar": "^3.5.3",
    "prettier": "^3.0.0",
    "standard": "^17.1.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/sensible": "^5.2.0",
    "ajv": "^8.12.0",
    "fastify": "^4.19.2",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.5.0",
    "globby": "^13.2.2",
    "pino-pretty": "^10.0.0"
  },
  "packageManager": "pnpm@8.6.6"
}
